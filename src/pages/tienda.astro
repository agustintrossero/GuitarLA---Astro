---
import Layout from '../layouts/Layout.astro';

const url = `${import.meta.env.PUBLIC_API_URL}/api/guitarras`
const respuesta = await fetch(url)
const {data} = await respuesta.json()
//console.log(data)
---

<Layout title="Tienda" description="Pagina de Venta de Guitarras">
	<main class="contenedor">
		<div class="heading">Nuestra Colecci√≥n</div>
		<div class="grid">
			{data?.map(guitarra =>(
				<h1>{guitarra.attributes.nombre}</h1>
			))}
		</div>
	</main>
</Layout>

